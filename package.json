{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc && cp src/municipios.csv dist/municipios.csv",
    "start": "node dist/server.js",
    "postbuild": "cp -r src/public dist/ 2>/dev/null || true",
    "vercel-build": "npm run build",
    "load-municipios": "ts-node src/scripts/loadMunicipios.ts",
    "test-municipios": "ts-node src/scripts/testMunicipios.ts",
    "view-municipios": "ts-node src/scripts/viewMunicipios.ts",
    "view-all-municipios": "ts-node src/scripts/viewMunicipios.ts -- --all",
    "test-edital-analysis": "ts-node src/scripts/testEditalAnalysis.ts",
    "test-direct-agents": "ts-node src/scripts/testDirectAgents.ts",
    "test-workflow": "ts-node src/scripts/testEditalWorkflow.ts",
    "test-analysis-route": "ts-node src/scripts/testAnalysisRoute.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.24",
    "@mastra/core": "^0.16.3",
    "@mastra/libsql": "^0.14.1",
    "@mastra/memory": "^0.15.1",
    "@mastra/pg": "^0.15.2",
    "@mastra/pinecone": "^0.11.8",
    "@mastra/rag": "^1.2.4",
    "@pinecone-database/pinecone": "^6.1.2",
    "@sparticuz/chromium": "^138.0.2",
    "@supabase/supabase-js": "^2.56.0",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.1",
    "@types/puppeteer": "^5.4.7",
    "@types/yauzl": "^2.10.3",
    "ai": "latest",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^5.20.1",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^3.2.0",
    "puppeteer-core": "^24.22.2",
    "redis": "^5.8.1",
    "yauzl": "^3.2.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.5.0",
    "@types/pdf-parse": "^1.1.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  }
}
